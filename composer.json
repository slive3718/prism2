{
  "name": "abstractsuite/afs",
  "type": "project",
  "description": "Web application of Abstract Suite",
  "homepage": "http://localhost/owpm2.com",
  "license": "All rights reserved",
  "require": {
    "php": "^7.3 || ^8.0",
    "codeigniter4/framework": "4.3.3",
    "guzzlehttp/guzzle": "^7.4",
    "kint-php/kint": "^4.2",

    "phpoffice/phpspreadsheet": "^1.28",
    "phpoffice/phpword": "^0.18.0",
    "ext-json": "*",
    "firebase/php-jwt": "^6.2",
    "nyholm/psr7": "^1.8",
    "php-http/guzzle7-adapter": "^1.0",
    "phpmailer/phpmailer": "^6.9",
    "ckeditor/ckeditor": "^5"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9",
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^9.6",
    "firebase/php-jwt": "^6.2",
    "phpoffice/phpspreadsheet": "^1.28",
    "phpoffice/phpword": "^0.18.0"
  },
  "suggest": {
    "ext-fileinfo": "Improves mime type detection for files"
  },
  "autoload": {
    "psr-4": {},
    "exclude-from-classmap": [
      "**/Database/Migrations/**"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Support\\": "tests/_support"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "support": {},
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
